{
  "name": "puppet-k8s",
  "version": "0.0.1",
  "author": "Vox Pupuli",
  "summary": "Install and manage plain Kubernetes installs",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-k8s",
  "project_page": "https://github.com/voxpupuli/puppet-k8s",
  "issues_url": "https://github.com/voxpupuli/puppet-k8s/issues",
  "dependencies": [
    {
      "name": "puppet-archive",
      "version_requirement": ">= 4.0.0 < 7.0.0"
    },
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 5.0.0 < 9.0.0"
    },
    {
      "name": "puppet-kmod",
      "version_requirement": ">= 3.2.0 < 4.0.0"
    },
    {
      "name": "puppet-augeasproviders_core",
      "version_requirement": ">= 2.4.0 < 4.0.0"
    },
    {
      "name": "puppet-augeasproviders_sysctl",
      "version_requirement": ">= 2.6.2 < 3.0.0"
    },
    {
      "name": "puppet-systemd",
      "version_requirement": ">= 2.0.0 < 4.0.0"
    },
    {
      "name": "puppetlabs-firewall",
      "version_requirement": ">= 4.0.0 < 6.0.0"
    },
    {
      "name": "puppet-firewalld",
      "version_requirement": ">= 4.5.0 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04",
        "20.10",
        "22.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0"
    }
  ]
}
